<tunnel>
    <ipsec>
        <entry name="{{ tunnel_name }}">
            <auto-key>
                <ike-gateway>
                  <entry name="{{ ike_gateway_name }}"/>
                </ike-gateway>
            </auto-key>
            <tunnel-monitor>
                <enable>no</enable>
              </tunnel-monitor>
            <tunnel-interface>{{ tunnel_interface }}</tunnel-interface>
        </entry>
    </ipsec>
</tunnel>